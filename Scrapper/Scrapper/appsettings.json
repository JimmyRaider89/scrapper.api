{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "SearchEngines": {
    "Google": {
      "Url": "https://www.google.com.au/search?q={0}&num={1}",
      "MatchUrls": [ "infotrack.com.au", "infotrackgo.com.au" ],
      "RecordsPerRequest": 100,
      "MaxMatchesPerRequest": 100
    },
    "Bing": {
      "Url": "https://www.bing.com/search?q={0}&count={1}&first={2}",
      "MatchUrls": [ "infotrack.com.au", "infotrackgo.com.au" ],
      "RecordsPerRequest": 40,
      "MaxMatchesPerRequest": 100
    }
  }
}
